<% if @reservation.canceled? %>
  <%= render partial: 'reservations/cancel', locals: {reservation: @reservation, restaurant: @restaurant} %>
<% else %>  
  <%= turbo_frame_tag dom_id(@reservation) do %>
    <%= render partial: 'reservations/reservated', locals: {reservation: @reservation, restaurant: @restaurant} %>
  <% end %>
<% end %>
